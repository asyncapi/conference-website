// Section Swiper Slider
section.section.section-swiper-absoulte.context-dark.text-center( data-triangle=".section-swiper-content-overlay" )


  // Swiper Content
  .section-swiper-content.section-swiper-content-sm
    .container
      .block-promo.block-center
        img(src='/images/asyncapi-horizontal-white.svg' style='height: 100px; margin-bottom: 20px;')
        h3.wow( data-splitting ) Hackathon & Conference
        ul.list-inline.list-inline-md
          li
            .unit.unit-spacing-xs.align-items-center.wow( data-splitting )
              .unit-left.line-height-reset: +svg-icon('euro')(class='svg-icon-sm svg-icon-primary')
              .unit-body: h5.text-spacing-100: span.big Free
          li
            .unit.unit-spacing-xs.align-items-center.wow( data-splitting )
              .unit-left.line-height-reset: +svg-icon('earth-globe')(class='svg-icon-sm svg-icon-primary')
              .unit-body: h5.text-spacing-100: span.big Online
        ul.list-inline.list-inline-md
          li
            .unit.unit-spacing-xs.align-items-center.wow( data-splitting )
              .unit-left.line-height-reset: +svg-icon('small-calendar')(class='svg-icon-sm svg-icon-primary')
              .unit-body: h5.text-spacing-100: span.big: time(datetime='2021-10-01') Hackathon October 1 - 31, 2021
          li
            .unit.unit-spacing-xs.align-items-center.wow( data-splitting )
              .unit-left.line-height-reset: +svg-icon('small-calendar')(class='svg-icon-sm svg-icon-primary')
              .unit-body: h5.text-spacing-100: span.big: time(datetime='2021-11-16') Conference November 16-18, 2021
        .row
          .col-md-6.offset-md-3
            h4(style='font-weight:normal;') Conference Registration
            a.button.button-primary.box-with-triangle-right.wow.fadeScale( href='https://zoom.us/webinar/register/9416323899487/WN_pv8NDGIoSg2CnF9qsoptWw' target="_blank" data-triangle=".button-overlay" )
              span Register for free
              span.button-overlay
        //- .row
        //-   .col-md-6.offset-md-3
        //-     h4(style='font-weight:normal;') CFP is still open!
        //-     a.button.button-secondary.box-with-triangle-right.wow.fadeScale( href='https://linuxfoundation.smapply.io/prog/asyncapi_conference_2021/' target="_blank" data-triangle=".button-overlay" )
        //-       span Apply Until October 17th!
        //-       span.button-overlay
  // Swiper Slider Absolute
  .swiper-container.swiper-slider.swiper-slider-1( data-loop='true',  data-simulate-touch='false' data-autoplay='8500' data-direction='horizontal' data-effect='fade' )
    .swiper-wrapper
      // Swiper Slide 01
      .swiper-slide( data-slide-bg='images/slide-01-1894x1042.jpg')
